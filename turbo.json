{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "env": ["GOOGLE_CLIENT_ID","GOOGLE_CLIENT_SECRET","BASE_URL_API"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "start": {
      "dependsOn": [],
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "env": ["GOOGLE_CLIENT_ID","GOOGLE_CLIENT_SECRET","BASE_URL_API"]
    }
  }
}
